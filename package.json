{
  "name": "habit-geek",
  "homepage": "https://rafaelaudy.github.io/habit-geek",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.24",
    "@fortawesome/free-solid-svg-icons": "^5.11.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@rafael.audy/habit-geek-utils": "^1.0.5",
    "@reach/router": "^1.2.1",
    "bootstrap": "^4.3.1",
    "node-sass": "^4.12.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "react-scripts": "3.1.1",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "cypress": "^3.5.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "gh-pages": "^2.1.1",
    "start-server-and-test": "^1.10.6"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -d build",
    "test": "react-scripts test",
    "test:functional": "cypress open",
    "test:ci": "CI=true yarn test && start-server-and-test start http://localhost:3000 'cypress run'",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/*Container.{js,jsx}",
      "!src/**/*Mock*.{js,jsx}",
      "!src/index.js",
      "!src/store/index.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(@rafael.audy/habit-geek-utils)/)"
    ]
  }
}
